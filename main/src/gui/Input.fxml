<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="inputDialog" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="gui.MainController">
    <children>
        <HBox>
            <children>
                <Text fill="#202220" strokeType="OUTSIDE" strokeWidth="0.0" text="From" wrappingWidth="62.6640625">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Text>
                <TextField fx:id="fromField" prefHeight="25.0" prefWidth="227.0"/>
                <Button mnemonicParsing="false" onAction="#fromBrowse" text="Browse"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <VBox.margin>
                <Insets top="20.0"/>
            </VBox.margin>
        </HBox>
        <HBox>
            <children>
                <Text fill="#131413" strokeType="OUTSIDE" strokeWidth="0.0" text="To" wrappingWidth="62.6640625">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Text>
                <TextField fx:id="toField" prefHeight="25.0" prefWidth="227.0"/>
                <Button mnemonicParsing="false" onAction="#toBrowse" text="Browse"/>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="91.0" prefWidth="345.0">
            <children>
                <Button mnemonicParsing="false" onAction="#addInput" prefHeight="35.0" prefWidth="82.0" text="Add"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </HBox>
    </children>
</VBox>
